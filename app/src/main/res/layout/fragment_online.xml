<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.OnlineFragment"
    android:background="#FFFFFF">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_below="@+id/tab_layout"
        android:layout_alignBottom="@+id/view_pager"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="-160dp"
        android:orientation="vertical">

        <fragment
            android:id="@+id/fragmentbanner"
            android:name="com.example.music_player.Fragment.Banner"
            android:layout_width="match_parent"
            android:layout_height="160dp">

        </fragment>

        <fragment
            android:id="@+id/fragmentplaylist"
            android:name="com.example.music_player.Fragment.F_Playlist"
            android:layout_width="match_parent"
            android:layout_marginTop="5dp"
            android:layout_height="200dp">
        </fragment>
    </LinearLayout>
</FrameLayout>